	( ... )

- externs:
	extern puts ( s -- i )
	: type ( s -- ) puts drop ;

- polymorphic funcs:
	: 1drop swap drop swap ;
	drop '1 '2 --
		[ '1 '2 ]:1

- if:
	do inlines and add branch words
	: if ,branch >r ; inline
	: else r> ,label ,jump >r ; inline
	: endif r> ,label ; inline

- inline:
	test 1 2 + ;

- interactive top loop

